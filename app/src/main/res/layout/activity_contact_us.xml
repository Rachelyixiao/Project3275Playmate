<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ContactUs" android:background="@color/backgroundcolor3">

    <ImageView
            android:layout_width="145dp"
            android:layout_height="22dp" app:srcCompat="@drawable/logo" android:id="@+id/imgLogo"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="133dp" android:layout_marginStart="133dp" android:layout_marginEnd="133dp"
            android:layout_marginRight="133dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp" android:background="#00FFFFFF"/>
    <ImageView
            android:layout_width="117dp"
            android:layout_height="241dp" app:srcCompat="@drawable/ins" android:id="@+id/imageView2"
            android:background="#00FFFFFF"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="81dp"
            app:layout_constraintEnd_toStartOf="@+id/imageView3" android:layout_marginEnd="11dp"
            android:layout_marginTop="29dp" app:layout_constraintTop_toBottomOf="@+id/textView31"/>
    <ImageView
            android:layout_width="140dp"
            android:layout_height="248dp" app:srcCompat="@drawable/whatapp" android:id="@+id/imageView3"
            android:background="#00FFFFFF" android:layout_marginStart="11dp"
            app:layout_constraintStart_toEndOf="@+id/imageView2" android:layout_marginEnd="62dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="22dp"
            app:layout_constraintTop_toBottomOf="@+id/textView31"/>
    <TextView
            android:text="Scan to contact our online representatives below:"
            android:layout_width="271dp"
            android:layout_height="49dp" android:id="@+id/textView31"
            android:background="#00FFFFFF" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="48dp" android:layout_marginEnd="49dp"
            app:layout_constraintEnd_toEndOf="parent" android:textSize="20sp"
            app:layout_constraintHorizontal_bias="0.511" android:layout_marginTop="73dp"
            app:layout_constraintTop_toBottomOf="@+id/imgLogo"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/home" android:id="@+id/homeiconContact"
            android:background="#00FFFFFF"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="58dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="21dp"
            app:layout_constraintEnd_toStartOf="@+id/contacticonContact" android:layout_marginEnd="59dp"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/contact" android:id="@+id/contacticonContact"
            android:background="#00FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp"
            app:layout_constraintStart_toEndOf="@+id/homeiconContact" app:layout_constraintEnd_toStartOf="@+id/settingiconContact"
    />
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/setting" android:id="@+id/settingiconContact"
            android:background="#00FFFFFF" android:layout_marginStart="59dp"
            app:layout_constraintStart_toEndOf="@+id/contacticonContact" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="58dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>