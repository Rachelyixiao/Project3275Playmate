<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Homepage.ExpertMainPage" android:background="@drawable/back">
    <ImageView
            android:layout_width="145dp"
            android:layout_height="22dp" app:srcCompat="@drawable/logo" android:id="@+id/imgLogo"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="133dp" android:layout_marginStart="133dp" android:layout_marginEnd="133dp"
            android:layout_marginRight="133dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="24dp" android:background="#00FFFFFF"/>

    <TextView
            android:text="@string/UserMainText1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView22"
            android:textSize="34sp" android:textColor="@color/white" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="127dp" android:layout_marginEnd="143dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="64dp"
            android:background="#00FFFFFF"
            android:textStyle="bold" app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/ExpertName"
            android:background="#00FFFFFF" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="126dp"/>
    <TextView
            android:text="@string/UserMainText2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView24"
            android:textColor="@color/white" android:textSize="24sp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="148dp" android:layout_marginEnd="173dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            android:background="#00FFFFFF"
            android:layout_marginTop="93dp" app:layout_constraintTop_toBottomOf="@+id/textView22"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/ExpertBalance"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="250dp" android:background="#00FFFFFF"/>

    <TextView
            android:text="@string/ratingString"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white" android:textSize="24sp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="148dp" android:layout_marginEnd="173dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:background="#00FFFFFF"
            android:layout_marginTop="55dp" app:layout_constraintTop_toBottomOf="@+id/textView24"
            android:id="@+id/textView29"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/ExpertRating"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="343dp" android:background="#00FFFFFF"/>
    <Button
            android:text="@string/uploadProfilebtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/uploadProfilesLink"
            app:backgroundTint="@color/buttonColor" android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="127dp"
            android:layout_marginEnd="115dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="127dp"
            app:layout_constraintTop_toBottomOf="@+id/textView29"/>
    <Button
            android:text="@string/uploadHoursLink"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/uploadHoursLink"
            android:background="@color/buttonColor"
            app:backgroundTint="@color/buttonColor" android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="133dp"
            android:layout_marginEnd="128dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/uploadProfilesLink" app:layout_constraintHorizontal_bias="0.0"/>
    <Button
            android:text="@string/getWagelink"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/getWagelink"
            app:backgroundTint="@color/buttonColor" android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="127dp"
            android:layout_marginEnd="119dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="25dp"
            app:layout_constraintTop_toBottomOf="@+id/uploadHoursLink"/>

    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/home" android:id="@+id/homeiconCustomerMain"
            android:background="#FFFFFF"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="58dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="21dp"
            app:layout_constraintEnd_toStartOf="@+id/contacticonCustomerMain" android:layout_marginEnd="59dp"/>
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/contact" android:id="@+id/contacticonCustomerMain"
            android:background="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp"
            app:layout_constraintStart_toEndOf="@+id/homeiconCustomerMain"
            app:layout_constraintEnd_toStartOf="@+id/settingiconCustomerMain"
    />
    <ImageView
            android:layout_width="45dp"
            android:layout_height="45dp" app:srcCompat="@drawable/setting" android:id="@+id/settingiconCustomerMain"
            android:background="#FFFFFF" android:layout_marginStart="59dp"
            app:layout_constraintStart_toEndOf="@+id/contacticonCustomerMain" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="20dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="58dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>